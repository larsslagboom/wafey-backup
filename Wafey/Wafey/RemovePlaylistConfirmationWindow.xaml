<Window x:Class="Wafey.RemovePlaylistConfirmationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wafey"
        mc:Ignorable="d"
        Title="" Height="150" Width="350"
        ResizeMode="CanMinimize" Icon="Images/Wafey logo.png">
    <Grid Background="#FF2B2B2B">
        <Button x:Name="ButtonConfirm" Content="Confirm" HorizontalAlignment="Left" Margin="257,89,0,0" VerticalAlignment="Top" Width="75" Click="ButtonConfirm_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,89,0,0" Click="ButtonCancel_Click"/>
        <TextBlock x:Name="TextblockConfirmation" HorizontalAlignment="Left" Margin="10,20,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Foreground="#FF3594BD" Width="322" Height="38"/>

    </Grid>
</Window>
